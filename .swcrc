{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"dynamicImport": true,
			"tsx": true
		},
		"target": "es5",
		"loose": true
	},
	"module": {
		"type": "es6"
	},
	"isModule": true,
	"env": {
		"include": ["transform-async-to-generator", "proposal-object-rest-spread", "proposal-optional-chaining"]
	}
}
